{
    "name": "chevere/reference-md",
    "description": "Generates markdown docs for Chevere flavours",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Rodolfo Berrios",
            "email": "rodolfo@chevere.org"
        }
    ],
    "require": {
        "chevere/chevere": "dev-main",
        "phpdocumentor/reflection-docblock": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "Chevere\\ReferenceMd\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symplify/easy-coding-standard": "^9.0"
    },
    "repositories": {
        "dev-package": {
            "type": "path",
            "url": "/home/rodolfo/git/chevere/chevere",
            "options": {
                "symlink": true
            }
        }
    }
}
